{
  "extends": ["@alastair-smith/tsconfig"],
  "compilerOptions": {
    "jsx": "preserve",
    "paths": {
      "@/public/*": ["./public/*"],
      "@/*": ["./src/*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"]
}
