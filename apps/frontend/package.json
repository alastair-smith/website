    "@opentelemetry/semantic-conventions": "^1.25.1",
    "@types/node": "20.2.3",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "next": "14.1.4",
    "postcss": "8.4.38",
    "react": "18.2.0",
