{
  "name": "website",
  "version": "2.0.0",
  "description": "Personal website for Alastair Smith",
  "type": "module",
  "scripts": {
    "build": "node build.js",
    "dev": "npm run build && npm run start",
    "start": "node local.js",
    "linter": "standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alastair-smith/website.git"
  },
  "author": "Alastair Smith",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alastair-smith/website/issues"
  },
  "homepage": "https://alsmith.dev/",
  "devDependencies": {
    "copy-dir": "1.3.0",
    "express": "4.17.1",
    "gray-matter": "4.0.2",
    "highlight.js": "11.5.1",
    "highlightjs-terraform": "git+https://github.com/highlightjs/highlightjs-terraform.git#e5e42f6b9ee682f659a7d00d5218204fac7b04a9",
    "html-minifier": "4.0.0",
    "nunjucks": "3.2.2",
    "parse": "3.4.0",
    "rehype-autolink-headings": "6.1.0",
    "rehype-external-links": "1.0.1",
    "rehype-highlight": "5.0.2",
    "rehype-rewrite": "3.0.6",
    "rehype-slug": "5.0.0",
    "rehype-stringify": "9.0.2",
    "remark": "14.0.1",
    "remark-rehype": "10.0.1",
    "sass": "1.32.5",
    "standard": "16.0.4",
    "unified": "10.1.0"
  },
  "standard": {
    "ignore": [
      "build"
